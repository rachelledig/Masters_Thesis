  author = {Britten, Gregory L. and Mohajerani, Yara and Primeau, Louis and Aydin, Murat and Garcia, Catherine and Wang, Wei-Lei and Pasquier, Beno{\^i}t and Cael, B. B. and Primeau, Fran{\c c}ois W.},
