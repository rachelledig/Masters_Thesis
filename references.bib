  langid = {english},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Geophysics;Solid Earth sciences Subject\_term\_id: geophysics;solid-earth-sciences},
