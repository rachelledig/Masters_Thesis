@article{portmannMIRCA2000GlobalMonthly2010a,
  title = {{{MIRCA2000}}\textemdash{{Global}} Monthly Irrigated and Rainfed Crop Areas around the Year 2000: A New High-Resolution Data Set for Agricultural and Hydrological Modeling},
  shorttitle = {{{MIRCA2000}}\textemdash{{Global}} Monthly Irrigated and Rainfed Crop Areas around the Year 2000},
