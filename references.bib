@article{beckerDescriptionGlobalLandsurface2013,
  title = {A Description of the Global Land-Surface Precipitation Data Products of the {{Global Precipitation Climatology Centre}} with Sample Applications Including Centennial (Trend) Analysis from 1901\textendash Present},
  author = {Becker, A. and Finger, P. and {Meyer-Christoffer}, A. and Rudolf, B. and Schamm, K. and Schneider, U. and Ziese, M.},
  year = {2013},
  month = feb,
  journal = {Earth System Science Data},
  volume = {5},
  number = {1},
  pages = {71--99},
  publisher = {{Copernicus GmbH}},
  issn = {1866-3508},
  doi = {10.5194/essd-5-71-2013},
  abstract = {{$<$}p{$><$}strong class="journal-contentHeaderColor"{$>$}Abstract.{$<$}/strong{$>$} The availability of highly accessible and reliable monthly gridded data sets of global land-surface precipitation is a need that was already identified in the mid-1980s when there was a complete lack of globally homogeneous gauge-based precipitation analyses. Since 1989, the Global Precipitation Climatology Centre (GPCC) has built up its unique capacity to assemble, quality assure, and analyse rain gauge data gathered from all over the world. The resulting database has exceeded 200 yr in temporal coverage and has acquired data from more than 85 000 stations worldwide. Based on this database, this paper provides the reference publication for the four globally gridded monthly precipitation products of the GPCC, covering a 111-yr analysis period from 1901\&ndash;present. As required for a reference publication, the content of the product portfolio, as well as the underlying methodologies to process and interpolate are detailed. Moreover, we provide information on the systematic and statistical errors associated with the data products. Finally, sample applications provide potential users of GPCC data products with suitable advice on capabilities and constraints of the gridded data sets. In doing so, the capabilities to access El Ni\~no\&ndash;Southern Oscillation (ENSO) and North Atlantic Oscillation (NAO) sensitive precipitation regions and to perform trend analyses across the past 110 yr are demonstrated. The four gridded products, i.e. the Climatology (CLIM) V2011, the Full Data Reanalysis (FD) V6, the Monitoring Product (MP) V4, and the First Guess Product (FG), are publicly available on easily accessible latitude/longitude grids encoded in zipped clear text ASCII files for subsequent visualization and download through the GPCC download gate hosted on {$<$}a href="ftp://ftp.dwd.de/pub/data/gpcc/html/download\_gate.html"target="\_blank"{$>$}ftp://ftp.dwd.de/pub/data/gpcc/html/download\_gate.html{$<$}/a{$>$} by the Deutscher Wetterdienst (DWD), Offenbach, Germany. Depending on the product, four (0.25\&deg;, 0.5\&deg;, 1.0\&deg;, 2.5\&deg; for CLIM), three (0.5\&deg;, 1.0\&deg;, 2.5\&deg;, for FD), two (1.0\&deg;, 2.5\&deg; for MP) or one (1.0\&deg; for FG) resolution is provided, and for each product a DOI reference is provided allowing for public user access to the products. A preliminary description of the scope of a fifth product \&ndash; the Homogenized Precipitation Analysis (HOMPRA) \&ndash; is also provided. Its comprehensive description will be submitted later in an extra paper upon completion of this data product. DOIs of the gridded data sets examined are as follows: {$<$}br{$><$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/CLIM\_M\_V2011\_025"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/CLIM\_M\_V2011\_025{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/CLIM\_M\_V2011\_050"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/CLIM\_M\_V2011\_050{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/CLIM\_M\_V2011\_100"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/CLIM\_M\_V2011\_100{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/CLIM\_M\_V2011\_250"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/CLIM\_M\_V2011\_250{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/FD\_M\_V6\_050"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/FD\_M\_V6\_050{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/FD\_M\_V6\_100"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/FD\_M\_V6\_100{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/FD\_M\_V6\_250"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/FD\_M\_V6\_250{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/MP\_M\_V4\_100"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/MP\_M\_V4\_100{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/MP\_M\_V4\_250"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/MP\_M\_V4\_250{$<$}/a{$>$},{$<$}br{$>$} {$<$}a href="http://dx.doi.org/10.5676/DWD\_GPCC/FG\_M\_100"target="\_blank"{$>$}doi:10.5676/DWD\_GPCC/FG\_M\_100{$<$}/a{$>$}.{$<$}/p{$>$}},
  langid = {english},
  file = {/Users/rachelledig/Zotero/storage/926LKPJB/2013.html}
}

